{
    "manifest_version": 2,
    "name": "Chrome Extension",
    "description": "Check your tvseries progress here!",
    "version": "1.0",

    "icons": { "16": "/icon.png",
           "48": "/icon.png",
          "128": "/icon.png" },

    "browser_action": {
        "default_icon": "/icon.png",
        "default_popup": "/Popup/popup.html"
    },

    "permissions": [
    	"storage"
    ]
}
